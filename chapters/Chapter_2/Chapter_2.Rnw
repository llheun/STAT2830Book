  \begin{exercise}  % 1
  
    250 individuals were randomly selected and 195 of them do not smoke. What proportion of individuals are smokers?
	  \begin{enumerate}
	  \samepage
	  \item 78
	  \samepage
	  \item 0.22
	  \samepage
	  \item 78\%
	  \samepage
	  \item 22\%
  	\end{enumerate}
  	\vspace{5mm}
    \framebox[5cm][l]{ Answer: }
  	
  \end{exercise}
  \vspace{2mm}
  \begin{solution}
    \begin{equation}
    (2.) p = \frac{250 - 195}{250} = 0.22
    \end{equation}
  \end{solution}
    
  	
	
  \begin{exercise} % 2
  
    250 individuals were randomly selected and 195 of them do not smoke.  What is the ratio of nonsmokers to smokers?
	  \begin{enumerate}
	  \item 0.78
	  \item 0.28
	  \item 3.55
	  \item 55
	  \end{enumerate}
	  \vspace{5mm}
    \framebox[5cm][l]{ Answer: }
	  
	\end{exercise}
	\vspace{2mm}
	\begin{solution}
    \begin{equation}
    (3.) ratio = \frac{195}{250 - 195} = 3.55
    \end{equation}
  \end{solution}

  \begin{exercise} % 3
  
    In a city of a population 25,000, there were 13 murders reported in the prior year.  What is the rate of murder in this city per 100,000?
	  \begin{enumerate}
	  \samepage
	  \item 5.2
	  \samepage
	  \item 52
	  \samepage
	  \item 5.24
	  \samepage
	  \item 0.00052
	  \end{enumerate}
	  \vspace{5mm}
    \framebox[5cm][l]{ Answer: }
    
	\end{exercise}
	\vspace{2mm}
	\begin{solution}
	  \begin{equation}
	  (2.)  rate = \frac{13}{25000} \times 100000 = 52 
	  \end{equation}
	\end{solution}
	
  \begin{exercise} % 4 
  
    In 1995 the percentage of individuals graduating from the Portage high school was 89 percent, in 2005, the percentage was 83 percent.  What is the percent change?
	  \begin{enumerate}
	  \samepage
	  \item $-6.7\%$
	  \samepage
	  \item $6.7\%$
	  \samepage
	  \item $7.2\%$
	  \samepage
	  \item $-7.2\%$
	  \end{enumerate}
	  \vspace{5mm}
    \framebox[5cm][l]{ Answer: }
    
	\end{exercise}
	\vspace{2mm}
	\begin{solution}
	  \begin{equation}
	    (1.)  PC = \frac{(83 - 89)}{89} \times 100 = -6.7\%
	  \end{equation}
	\end{solution}
	
