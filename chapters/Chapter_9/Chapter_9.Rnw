	\begin{enumerate}
	\item $H_0: \mu_1 = \mu_2$ vs. $H_A: \mu_1 < \mu_2$
	\item $H_0: P_1 = P_2$ vs. $H_A: P_1 < P_2$
	\item $H_0: \mu_1 = \mu_2$ vs. $H_0: \mu_1 > \mu_2$
	\item $H_0: \mu_1 = \mu_2$ vs. $H_0: \mu_1 \neq \mu_2$
	\item $H_0: \mu_1 <\mu_2$ vs. $H_0: \mu_1 > \mu_2$
	\end{enumerate}
	
